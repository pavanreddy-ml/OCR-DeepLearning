    BoxLayout:
		orientation: "vertical"
		size: root.width, root.height

		FloatLayout:

		    GridLayout:
		        cols: 2
		        pos_hint: {'x': 522/800, 'y': 1 - 49/520}
                size_hint: 273/800, 45/520
                background_normal: ''

                spacing:3


                ToggleButton:
                    id: Dataset1
                    text: 'Dataset1\nDenseNet201'
                    color: {'normal': (173/255, 191/255, 220/255, 1), 'down': (1, 1, 1, 1)} [self.state]
                    group: 'Dataset'
                    state: 'normal'
                    canvas.before:
                        Color:
                            rgba: {'normal': (45/255, 158/255, 194/255, 1), 'down': (0, 0, 0, 1)} [self.state]
                        Line:
                            width: {'normal': 1.2, 'down': 0.6} [self.state]
                            rectangle: self.x, self.y, self.width, self.height
                    background_normal: ''
                    background_color: {'normal': (27/255, 36/255, 52/255, 1), 'down': (227/255, 246/255, 240/255, 1)} [self.state]
                    on_press: root.click(self)
                    disabled: True

                ToggleButton:
                    id: Dataset2
                    text: 'Dataset2\nSimple Conv2D'
                    color: {'normal': (173/255, 191/255, 220/255, 1), 'down': (1, 1, 1, 1)} [self.state]
                    group: 'Dataset'
                    state: 'down'
                    canvas.before:
                        Color:
                            rgba: {'normal': (45/255, 158/255, 194/255, 1), 'down': (0, 0, 0, 1)} [self.state]
                        Line:
                            width: {'normal': 1.2, 'down': 0.6} [self.state]
                            rectangle: self.x, self.y, self.width, self.height
                    background_normal: ''
                    background_color: {'normal': (27/255, 36/255, 52/255, 1), 'down': (227/255, 246/255, 240/255, 1)} [self.state]
                    on_press: root.click(self)



